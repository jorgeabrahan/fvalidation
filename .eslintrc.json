{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "ignorePatterns": ["fvalidation.js"],
    "rules": {
        "no-extend-native": "off",
        "valid-typeof": "off",
        /* disabled no unused vars on typescript for generics */
        "@typescript-eslint/no-unused-vars" : "off"
    }
}
